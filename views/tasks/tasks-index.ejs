<% include ../partials/boilerplate %>

<a href="/tasks/new">Add a new task</a>

<% tasks.forEach(task => { %>
  <div class="task">
    <h2><%= task.title %>  <%= task.status %> </h2>
    <h3><%= task.category%></h3>
    <a href="/tasks/<%=task.id%>">See That Task</a>
  </div>
<% }) %>


<% include ../partials/end %>
